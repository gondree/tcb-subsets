<org.eventb.core.extendsContext name="'" org.eventb.core.target="TCB_Subsets"/>
<org.eventb.core.constant de.prob.symbolic.symbolicAttribute="false" name="(" org.eventb.core.comment="``The concept of a 'protected object' requires some interpretation'' (Sec 3, pg. 134)" org.eventb.core.identifier="protected_objs"/>
<org.eventb.core.axiom name=")" org.eventb.core.label="rel1" org.eventb.core.predicate="protected_objs ∈ TCBSUBSETS → ℙ(OBJECTS)"/>
